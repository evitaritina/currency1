                <!-- fire printing event in google analytics -->
                <script>

                    (function() {
                    var afterPrint = function() {
                    tracker = ga.getAll()[0];
                    tracker.send({
                    'hitType': 'event',
                    'eventCategory': 'print',
                    'eventAction': 'print',
                    'eventLabel': window.location.pathname,
                    'page': window.location.pathname
                    });
                    };
                    if (window.matchMedia) {
                    var mediaQueryList = window.matchMedia('print');
                    mediaQueryList.addListener(function(mql) {
                    if (!mql.matches) {
                    afterPrint();
                    }
                    });
                    }



                    var addSocialMediaTracking = function(){
                    var sl = document.querySelectorAll('ul.socialSharing>li');
                    for(var i=0;i<sl.length;i++)
                    {
                    sle = sl[i];
                    var slClass = sle.classList[0];

                    var a = function(s){
                    sle.onclick = function(e){
                    tracker = ga.getAll()[0];
                    tracker.send({
                    'hitType': 'event',
                    'eventCategory': 'share',
                    'eventAction': s,
                    'eventLabel': window.location.pathname,
                    'page': window.location.pathname
                    });

                    }
                    };
                    a(slClass);
                    }
                    }

                    addSocialMediaTracking();


                    window.onafterprint = afterPrint;
                    }());
                </script>

                <script async src="/shared/dist/js/main.min.js?4328" type="text/javascript"></script>
                <script>
                    var id = 'GTM-NVKFGW';
                    if(localStorage.cookieChoice && localStorage.cookieChoice.substr(0,3)=='acc'){
                    var jsCode = '(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({\'gtm.start\':' +
                    'new Date().getTime(),event:\'gtm.js\'});var f=d.getElementsByTagName(s)[0],'+
                    'j=d.createElement(s),dl=l!=\'dataLayer\'?\'&l=\'+l:\'\';j.async=true;j.src='+
                    '\'//www.googletagmanager.com/gtm.js?id=\'+i+dl;f.parentNode.insertBefore(j,f);'+
                    '})(window,document,\'script\',\'dataLayer\',\'' +id+ '\');';

                    eval(jsCode);
                    }
                </script>


